{
  "name": "pms-components",
  "version": "0.1.0",
  "private": true,
  "description": "Biblioteca simples de componentes utilizada na Prefeitura Municipal de Santos",
  "main": "./dist/pms-components.common.js",
  "publishConfig": {
    "@rudsan:registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:npm": "vue-cli-service build --target lib --name pms-components src/index.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "bootstrap": "^3.4.1",
    "core-js": "^3.8.3",
    "jquery": "^3.7.0",
    "vue": "^2.6.14",
    "vue-router": "^2.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "files": [
    "dist/*"
  ]
}
